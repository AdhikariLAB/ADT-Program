***,F+H2 
! $F$ : basename to be substituted
! $R$ : record name
! $S$ : starting record name if it is available
file,2,$F$.wfu
punch,$F$.pun

basis=aug-cc-pvtz;

symmetry,x

geometry=$F$.xyz

{mcscf;
occ,9,2;closed,1,0;
wf,11,1,1;state,2;
wf,11,2,1;
maxiter,40;
start,$S$;
orbital,$R$;}

$resvars = [E1AP,E2AP,E1APP]

show,energy
table,resvars,energy
save,$F$.res,new
title 'results: Energies ---  $F$'
